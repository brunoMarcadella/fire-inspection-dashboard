{
  "name": "fire-inspection-api",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "start": "node --enable-source-maps dist/index.js",
    "build": "tsc -p .",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "prepare": "prisma generate && npm run build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "express": "^4.19.2",
    "zod": "^3.23.8",
    "@prisma/client": "^5.16.2"
  },
  "devDependencies": {
    "prisma": "^5.16.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
